<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/car_wash_logo0"
        android:label="@string/app_name"
        android:roundIcon="@drawable/car_wash_logo0"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShineAuto">

        <activity
            android:name=".ui.LoginActivity"
            android:theme="@style/Theme.App.Starting"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.SignupActivity" />

        <activity
            android:name=".ui.AdminActivity"
            android:exported="false" />

        <activity
            android:name=".ui.ProviderActivity"
            android:exported="false" />

        <activity
            android:name=".ui.CustomerActivity"
            android:exported="false" />
        <activity
            android:name=".ui.AboutUsActivity"
            android:parentActivityName=".ui.CustomerActivity" />
        <activity
            android:name=".ui.FaqsActivity"
            android:parentActivityName=".ui.CustomerActivity" />
        <activity
            android:name=".ui.ContactUsActivity"
            android:parentActivityName=".ui.CustomerActivity" />
    </application>

</manifest>